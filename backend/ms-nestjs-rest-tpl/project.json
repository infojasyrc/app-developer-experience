{
  "name": "nestjs-rest-tpl",
  "sourceRoot": "backend/ms-nestjs-rest-tpl",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "outputs": ["{projectRoot}/dist"]
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm start"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test:unit:coverage"
      }
    }
  }
}