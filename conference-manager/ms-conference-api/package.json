{
  "name": "@appdevexp/msa-conference-api",
  "version": "1.0.0",
  "description": "Conference Manager API",
  "scripts": {
    "build": "nest build",
    "lint": "npx eslint .",
    "prepare": "husky",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "test:ci": "yarn test --coverage",
    "test:coverage": "yarn test --coverage",
    "test:unit-tests:v1": "nyc --reporter=lcov ava",
    "test:unit-tests:v2": "jest",
    "test:watch:v1": "ava --watch",
    "test:watch:v2": "jest --watch",
    "dedupe": "yarn-deduplicate yarn.lock",
    "security:audit": "audit-ci --high --critical --report-type summary --config audit-ci.json",
    "security:fix": "yarn upgrade && yarn install || true"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": "18"
  },
  "private": true,
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-angular": "^17.4.2",
    "@nestjs/testing": "^10.3.1",
    "@types/jest": "^29.5.11",
    "@types/semver": "^7.7.0",
    "@types/uuid": "^9.0.8",
    "audit-ci": "^7.1.0",
    "ava": "^5.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.39.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "mock-req-res": "^1.2.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^15.0.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.2",
    "yamljs": "^0.3.0",
    "yarn-deduplicate": "^6.0.2"
  },
  "dependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.4",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/testing": "^10.3.1",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-strategy": "^0.2.38",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "express-multipart-file-parser": "^0.1.2",
    "firebase": "^9.21.0",
    "firebase-admin": "^12.0.0",
    "joi": "^17.9.2",
    "js-yaml": "^4.1.0",
    "json2csv": "^5.0.1",
    "mongodb": "^5.6.0",
    "mongodb-memory-server": "^9.1.6",
    "mongoose": "^8.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.1",
    "typescript": "^5.3.3",
    "uuid": "^9.0.0",
    "xhr2": "^0.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint && yarn run test:unit-tests:v1 && test:unit-tests:v2"
    }
  },
  "nodemonConfig": {
    "ignore": [
      "tests/*"
    ],
    "verbose": false
  }
}
