{
  "name": "@conference-manager/ms-conference-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:ci": "vitest --run --coverage",
    "commit": "git-cz",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "version:auto": "node scripts/auto-version.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.67.3",
    "next": "^15.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/eslintrc": "^3",
    "@storybook/addon-essentials": "^8.6.7",
    "@storybook/addon-onboarding": "^8.6.7",
    "@storybook/blocks": "^8.6.7",
    "@storybook/experimental-addon-test": "^8.6.7",
    "@storybook/experimental-nextjs-vite": "^8.6.7",
    "@storybook/react": "^8.6.7",
    "@storybook/test": "^8.6.7",
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-query-devtools": "^5.69.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-query": "^1.2.8",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.0.9",
    "@vitest/coverage-v8": "^3.0.9",
    "commitizen": "^4.3.1",
    "conventional-changelog-cli": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.2",
    "eslint-plugin-storybook": "^0.11.6",
    "git-cz": "^4.9.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "playwright": "^1.51.1",
    "semver": "^7.7.1",
    "storybook": "^8.6.7",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vite": "^6.2.2",
    "vitest": "^3.0.9"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
